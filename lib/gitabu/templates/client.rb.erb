# frozen_string_literal: true

require_relative "api/v3/actions"

# Ruby gem that helps you work with Github API.
module Gitabu
  # Client that acts as a middleman to the API.
  class Client
  <% for @item in @items %>
    def self.<%= @item['method_name'] %>(<%= @item['params_key_value_flat'] %>)
      Gitabu::Api::V3::Actions.<%= @item['method_name'] %>(<%= @item['params_key_value_flat'] %>)
    end
  <% end %>
  end
end
